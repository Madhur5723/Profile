	    int sum=0;
	    for(int i=0;i<a.length();i++)
	    	int n=arr[i]-'0';
	    {
	    	if(a%2!=0)
	    	{
	    		sum+=a;
	    		
	    	}
	    }
	    
	    {
	    	
	    }
		

	}

}
